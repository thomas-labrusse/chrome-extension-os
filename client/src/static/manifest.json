{
	"name": "GC traits",
	"description": "Get ranks directly on page",
	"version": "1.0.0",
	"manifest_version": 3,
	"permissions": ["storage", "activeTab", "scripting"],
	"content_scripts": [
		{
			"matches": ["https://opensea.io/*"],
			"js": ["contentScript.js"]
		}
	],
	"action": {
		"default_title": "Change current page color",
		"default_popup": "popup.html",
		"default_icon": {
			"16": "logo.png",
			"32": "logo.png",
			"48": "logo.png",
			"128": "logo.png"
		}
	},
	"icons": {
		"16": "logo.png",
		"32": "logo.png",
		"48": "logo.png",
		"128": "logo.png"
	}
}
